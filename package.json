{
  "name": "ursiform",
  "version": "1.0.0",
  "description": "Natural language interface for Qdrant vector databases",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:run": "node scripts/test-runner.js",
    "test:run-lite": "npx jest tests/backend/simple.test.ts",
    "test:run-working": "npx jest tests/backend/config.test.ts tests/backend/simple.test.ts",
    "test:run-coverage": "node scripts/test-runner.js --coverage",
    "test:run-watch": "node scripts/test-runner.js --watch",
    "test:config": "node scripts/test-runner.js --suite=config",
    "test:startup": "node scripts/test-runner.js --suite=startup",
    "test:db": "node scripts/test-runner.js --suite=qdrant-db",
    "test:embedder": "node scripts/test-runner.js --suite=embedder",
    "test:nlp": "node scripts/test-runner.js --suite=nlp-query",
    "test:demo": "node scripts/demo-test-runner.js",
    "test:errors": "tsx lib/test-error-handling.ts",
    "test:errors-integration": "tsx lib/test-integration-errors.ts",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t ursiform .",
    "docker:run": "docker run -p 3000:3000 ursiform",
    "docker:up": "docker compose up --build",
    "docker:down": "docker compose down",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push --force-reset",
    "trpc:build": "tsc --noEmit && echo 'tRPC types are valid'"
  },
  "dependencies": {
    "@google/genai": "^0.3.1",
    "@prisma/client": "^5.22.0",
    "@qdrant/js-client-rest": "^1.7.0",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.79.0",
    "@tanstack/react-query-devtools": "^5.79.0",
    "@trpc/client": "^11.1.4",
    "@trpc/next": "^11.1.4",
    "@trpc/react-query": "^11.1.4",
    "@trpc/server": "^11.1.4",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.3.1",
    "jotai": "^2.12.5",
    "lucide-react": "^0.511.0",
    "next": "^15.3.2",
    "next-themes": "^0.4.6",
    "openai": "^4.24.1",
    "prisma": "^5.22.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.0",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.4.0-canary.51",
    "fastify": "^5.3.3",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
