{
  "name": "yearn-backend",
  "version": "1.0.0",
  "description": "Backend service with Fastify, TypeScript, and Qdrant vector database",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "up": "docker-compose up --build",
    "up-cloud": "docker-compose -f docker-compose.cloud.yml up --build",
    "docker-build": "tsc && docker build -t backend .",
    "clean": "docker-compose down",
    "clean-cloud": "docker-compose -f docker-compose.cloud.yml down"
  },
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.0",
    "zod": "^3.22.4",
    "@qdrant/js-client-rest": "^1.7.0",
    "@google/generative-ai": "^0.2.1",
    "openai": "^4.24.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
